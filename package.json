    "find-deprecated-rules:next": "pnpm find-deprecated-rules ./configs/next.js",
    "find-deprecated-rules:node": "pnpm find-deprecated-rules ./configs/node.js",
    "find-deprecated-rules:react": "pnpm find-deprecated-rules ./configs/react.js",
    "find-deprecated-rules:ts": "pnpm find-deprecated-rules ./configs/ts.js"
